{
  "name": "h11",
  "module": "src/h9.ts",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "bun run ./build.ts",
    "build1": "bun build ./src/h11.ts --outdir ./dist && bun run types",
    "types": "tsc --project tsconfig.types.json"
  },
  "exports": {
    ".": {
      "types": "./dist/types/h11.d.ts",
      "default": "./dist/h11.ts"
    },
    "./bun": {
      "types": "./dist/types/providers/bun.d.ts",
      "default": "./dist/bun.ts"
    },
    "./node": {
      "types": "./dist/types/providers/node.d.ts",
      "default": "./dist/bun.ts"
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "utftu": "^1.0.26"
  }
}
